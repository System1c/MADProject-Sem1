<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/conlayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".homefrag">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/tool"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="512dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back"
        tools:srcCompat="@drawable/back" />

    <ImageButton
        android:id="@+id/female"
        android:layout_width="111dp"
        android:layout_height="104dp"
        android:layout_marginStart="150dp"
        android:layout_marginTop="379dp"
        android:layout_marginEnd="150dp"
        android:layout_marginBottom="248dp"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/avatar" />

    <ImageButton
        android:id="@+id/perfume"
        android:layout_width="111dp"
        android:layout_height="104dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="507dp"
        android:layout_marginEnd="284dp"
        android:layout_marginBottom="120dp"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/shoe"
        tools:srcCompat="@drawable/perfume" />

    <ImageButton
        android:id="@+id/shoe"
        android:layout_width="111dp"
        android:layout_height="104dp"
        android:layout_marginStart="285dp"
        android:layout_marginTop="379dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="248dp"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        android:src="@drawable/shoe"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@drawable/shoe" />

    <ImageButton
        android:id="@+id/male"
        android:layout_width="111dp"
        android:layout_height="104dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="379dp"
        android:layout_marginEnd="284dp"
        android:layout_marginBottom="248dp"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/shirt" />

    <ImageButton
        android:id="@+id/watch"
        android:layout_width="111dp"
        android:layout_height="104dp"
        android:layout_marginStart="150dp"
        android:layout_marginTop="507dp"
        android:layout_marginEnd="150dp"
        android:layout_marginBottom="120dp"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/watch" />

    <ImageButton
        android:id="@+id/deals"
        android:layout_width="111dp"
        android:layout_height="104dp"
        android:layout_marginStart="285dp"
        android:layout_marginTop="507dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="120dp"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/label" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="374dp"
        android:layout_height="44dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="263dp"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="424dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/search"
        android:layout_width="210dp"
        android:layout_height="53dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="317dp"
        android:layout_marginEnd="101dp"
        android:layout_marginBottom="361dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_launcher_foreground" />
</androidx.constraintlayout.widget.ConstraintLayout>